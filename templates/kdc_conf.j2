[kdcdefaults]
    kdc_ports = {{ kerberos_ports | join(',') }}
    kdc_tcp_ports = {{ kerberos_tcp_ports | join(',') }}

[realms]
    {{ kerberos_realm }} = {
        database_name = {{ path_to_principal_database }}
        admin_keytab = {{ path_to_admin_keytab }}
        acl_file = {{ path_to_acl_file }}
        key_stash_file = {{ path_to_key_stash_file }}
        kdc_ports = {{ kerberos_ports | join(',') }}
        max_life = {{ kerberos_max_life }}
        max_renewable_life = {{ kerberos_max_renewable_life }}
        master_key_type = {{ kerberos_master_key_type }}
        #supported_enctypes = aes256-cts:normal aes128-cts:normal
        default_principal_flags = {{ kerberos_default_principal_flags }}
    }
